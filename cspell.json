{
        "version": "0.2",
        "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
        "enableGlobDot": true,
        "language": "en,es-ES,en-GB",
        "words": [
                "Artboard",
                "Bañón",
                "Cayetano",
                "Rfblr",
                "robert",
                "usecase"
        ],
        "useGitignore": true,
        "ignorePaths": [
                ".git/*",
                ".git/!{COMMIT_EDITMSG,EDITMSG}",
                ".git/*/**",
                ".vscode",
                ".mason",
                ".idea",
                ".github",
                ".gitignore",
                "cspell.json",
                "ios/*",
                "android/*",
                "linux/*",
                "macos/*",
                "windows/*",
                "android/*",
                "lib/theme/assets.dart",
                "web/.well-known/*",
                "*/README.md",
                "README.md",
                "lib/firebase_options.dart",
                ".dart_tool/*",
                ".flutter-plugins",
                ".flutter-plugins-dependencies",
                ".metadata",
                "build/*",
                "*.properties",
                "*.lock",
                "*.yaml",
                "*.g.dart",
                "mason-lock.json",
                "local_bricks/*",
                "scripts/*",
                "AUTHORS"
        ]
}
